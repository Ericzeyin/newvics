                        <p style="color: red;">Select one of the option to proceed ahead.</p>
                    </div>
                    <div class="ui column centered grid">
                        <div class="ui vertical buttons" style="margin-top: 50px;">
                            <button onclick="buttonClick(1)" class="ui toggle black button" style="margin-bottom: 20px;padding-left: 50px;padding-right: 50px;">Fine dinning</button>
                            <button onclick="buttonClick(2)" class="ui black button" style="margin-bottom: 20px;padding-left: 50px;padding-right: 50px;">Casual dining</button>
                            <button onclick="buttonClick(3)" class="ui black button" style="margin-bottom: 20px;padding-left: 50px;padding-right: 50px;">Cheap eats</button>
                        </div>
                        <div class="ui center aligned container">
                            <button onclick="question5(document.getElementById('clicked_button').value)" class="ui button" id="question5_next" style="margin-bottom: 20px;padding-left: 50px;padding-right: 50px;">
                                Next <i class="arrow circle right icon"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="ui vertical divider">
                <i class="circle outline icon"></i>
            </div>
        </div>

        @*Result*@
        <div class="ui segment" id="result" style="margin-top: 100px; margin-bottom: 100px;">
            <div class="ui two column very relaxed grid">
                <div class="ui column centered grid">
                    <div id="placehere" class="image" style="margin-bottom: 20px;">

                    </div>
                </div>
                <div class="column">
                    <div class="ui center aligned container">
                        <p style="font-size: 200%; color: black;">Congratulations!</p>
                    </div>
                    <div class="ui center aligned container">
                        <p style="font-size: 200%; color: black;">You are best suitable for: </p>
                    </div>
                    <br /><br />
                    <div class="ui center aligned container">
                        <p id="teamName" style="font-style: italic; font-size: 200%; color: black;"></p>
                    </div>
                </div>
            </div>
            <div class="ui vertical divider">
                <i class="circle outline icon"></i>
            </div>
        </div>

        <input type="hidden" name="clicked_button" id="clicked_button" value="" />

        <div class="row">
            <div class="col offset-lg-8 mx-auto text-center">
                <a class="btn btn-primary btn-xl sr-button" role="button" data-aos="zoom-in" data-aos-duration="400" data-aos-once="true" href="@Url.Action("Index", "Ranks")#quiz">Go Back</a>
                <a class="btn btn-danger btn-xl sr-button" role="button" data-aos="zoom-in" data-aos-duration="400" data-aos-once="true" href="@Url.Action("Question", "Main")">Play Again</a>
            </div>
        </div>
    </div>
